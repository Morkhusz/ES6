{
  "name": "es6",
  "version": "1.0.0",
  "description": "A project to learn modern javascript",
  "main": "index.js",
  "scripts": {
    "lint": "./node_modules/.bin/eslint src/*.js",
    "prepush": "npm run lint && npm run test:coverage",
    "test": "./node_modules/.bin/mocha tests/**/*.spec.js --require babel-register",
    "test:coverage": "nyc npm test"
  },
  "nyc": {
    "functions": 80,
    "lines": 80,
    "check-coverage": true,
    "reporter" : ["text", "html"],
    "exclude": ["tests/**"]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Morkhusz/ES6.git"
  },
  "keywords": [
    "EcmaScript6",
    "JavaScript"
  ],
  "author": "Morkhusz <jose-silvafilho@hotmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Morkhusz/ES6/issues"
  },
  "homepage": "https://github.com/Morkhusz/ES6#readme",
  "devDependencies": {
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.9.0",
    "husky": "^0.14.3",
    "mocha": "^5.1.0",
    "nyc": "^11.6.0"
  },
  "dependencies": {
    "eslint": "^4.18.2"
  }
}
